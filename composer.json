{
    "name": "cortezvini97/symfony-hybrid-views-bundle",
    "version": "1.0.1",
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/cortezvini97",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Cortez\\SymfonyHybridViews\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortez\\SymfonyHybridViews\\Tests\\":"tests/"
        }
    },
    "require": {
        "php":">=8.2",
        "symfony/console": "^7.0",
        "symfony/config": "^7.0",
        "symfony/yaml": "^7.0",
        "symfony/framework-bundle": "^7.0",
        "illuminate/view": "^9.0|^10.0|^11.0",
        "illuminate/config": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1",
        "symfony/test-pack": "^1.0",
        "symfony/phpunit-bridge": "^7.0",
        "phpspec/prophecy-phpunit": "^2.2"
    }
}
